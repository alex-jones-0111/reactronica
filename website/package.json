{
  "name": "reactronica-website",
  "version": "0.0.8",
  "description": "Documentation and examples for Reactronica audio library",
  "main": "index.js",
  "homepage": "https://reactronica.com",
  "scripts": {
    "dev": "next -p 4010",
    "start": "next start",
    "build": "next build",
    "test": "jest",
    "link": "npm link reactronica && npm link ../node_modules/react && npm link ../node_modules/react-dom",
    "deploy:prod": "vercel --prod",
    "deploy:staging": "vercel --local-config vercel.staging.json",
    "alias:staging": "now alias https://reactronica.unkleho.now.sh staging.reactronica.com",
    "graphql:schema": "apollo schema:download --endpoint=[GRAPHQL_URL]",
    "graphql:codegen": "apollo codegen:generate ./types/graphql.d.ts --addTypename --localSchemaFile schema.json --target typescript --outputFlat"
  },
  "author": {
    "name": "Kaho Cheung",
    "website": "https://twitter.com/unkleho"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.8",
    "graphql": "^15.5.0",
    "immer": "^6.0.1",
    "next": "^10.1.1",
    "normalize.css": "^8.0.1",
    "prism-react-renderer": "^0.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^2.7.0",
    "react-select": "^3.0.8",
    "reactronica": "^0.4.6",
    "recoil": "^0.2.0",
    "sass": "^1.32.8",
    "subscriptions-transport-ws": "^0.9.18",
    "webmidi": "^2.5.1"
  },
  "devDependencies": {
    "@testing-library/react": "^9.0.2",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.1",
    "autoprefixer": "^10.2.5",
    "dotenv": "^8.2.0",
    "hygen": "^4.0.8",
    "jest": "^26.6.3",
    "postcss": "^8.2.12",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "remark-slug": "^5.1.2",
    "stylelint": "^13.2.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "0.0.4",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "tailwindcss": "^2.1.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.3",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
